        "postcss": "^8.4.47",
        "prettier": "^3.4.2",
        "sass-embedded": "^1.80.4",
        "storybook": "^8.5.0",
        "stylelint": "^16.10.0",
        "stylelint-config-standard": "^37.0.0",
        "stylelint-config-standard-scss": "^14.0.0",
        "stylelint-prettier": "^5.0.2",
        "tailwindcss": "^3.4.14",
        "vite": "^6.0.7",
        "vite-plugin-vue-devtools": "^7.6.8"
